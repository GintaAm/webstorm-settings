<keymap version="1" name="Default copy" parent="Default for XWin">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl c" />
    <keyboard-shortcut first-keystroke="ctrl insert" />
    <keyboard-shortcut first-keystroke="windows" second-keystroke="c" />
  </action>
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="ctrl x" />
    <keyboard-shortcut first-keystroke="shift delete" />
    <keyboard-shortcut first-keystroke="windows" second-keystroke="x" />
  </action>
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="ctrl v" />
    <keyboard-shortcut first-keystroke="shift insert" />
    <keyboard-shortcut first-keystroke="windows" second-keystroke="v" />
  </action>
  <action id="CloseActiveTab" />
  <action id="CloseEditor">
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="windows" second-keystroke="c" />
  </action>
  <action id="Editor Cut">
    <keyboard-shortcut first-keystroke="windows" second-keystroke="x" />
  </action>
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="windows" second-keystroke="v" />
  </action>
  <action id="EditorSelectWord" />
  <action id="FindNext">
    <keyboard-shortcut first-keystroke="f3" />
  </action>
  <action id="GotoLine">
    <keyboard-shortcut first-keystroke="ctrl l" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="ctrl windows" second-keystroke="l" />
  </action>
  <action id="SaveAll">
    <keyboard-shortcut first-keystroke="ctrl s" />
    <keyboard-shortcut first-keystroke="windows" second-keystroke="s" />
  </action>
  <action id="SelectAllOccurrences">
    <keyboard-shortcut first-keystroke="shift ctrl alt g" />
  </action>
  <action id="SelectNextOccurrence">
    <keyboard-shortcut first-keystroke="ctrl g" />
  </action>
  <action id="UnselectPreviousOccurrence">
    <keyboard-shortcut first-keystroke="ctrl alt g" />
  </action>
  <action id="Vcs.Log.FocusTextFilter" />
  <action id="hg4idea.QGotoFromPatches" />
</keymap>